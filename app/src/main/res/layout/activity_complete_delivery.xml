<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.prasangafernando.couriertest3.CompleteDeliveryActivity">

    <Button
        android:id="@+id/btnScanCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvParcelID"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:text="Scan QR code"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="140dp" />

    <TextView
        android:id="@+id/tvParcelID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteY="85dp"
        tools:layout_editor_absoluteX="142dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="150dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvParcelID"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:layout_toLeftOf="@+id/btnScanCode"
        android:layout_toStartOf="@+id/btnScanCode"
        android:text="Parcel ID: " />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnScanCode"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp"
        android:text="Mark as Delivered" />
</RelativeLayout>
